<%= paginator.render do -%>
  <nav role="navigation" aria-label="pager">
    <ul class="pagination justify-content-center">
      <%= prev_page_tag %>
      <% each_page do |page| -%>
        <% if page.display_tag? -%>
          <%= page_tag page %>
        <% end -%>
      <% end -%>
      <% unless current_page.out_of_range? %>
        <%= next_page_tag %>
      <% end %>
    </ul>
  </nav>
<% end -%>
